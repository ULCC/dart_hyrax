<tr class="<%= dom_class(member) %> attributes">
  <td class="thumbnail">
    <%= render_thumbnail_tag member %>
  </td>
  <td class="attribute filename">
    <strong>filename: </strong>
    <%= link_to(member.link_name, contextual_path(member, @presenter)) %>
    <br/>
    <% if member.try(:content_version) %>
        <br/>
        <strong>content version: </strong>
        <%= content_version_string(member.try(:content_version)) %>
    <% end %>
    <% if member.try(:rights) %>
        <br/>
        <strong>license: </strong>
        <%= member.try(:rights).first %>
    <% end %>
  </td>
  <td class="attribute date_uploaded"><%= member.try(:date_uploaded) %></td>
  <td class="attribute permission"><%= member.permission_badge %></td>
  <td>
    <%= render 'actions', member: member %>
  </td>
</tr>

